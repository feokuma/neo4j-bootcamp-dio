# Neo4j Configuration
NEO4J_AUTH=neo4j/password
NEO4J_VERSION=5.15

# Memory Settings (adjust based on your system)
NEO4J_HEAP_INITIAL=512m
NEO4J_HEAP_MAX=2G
NEO4J_PAGECACHE=1G

# Network Settings
NEO4J_HTTP_PORT=7474
NEO4J_BOLT_PORT=7687

# Plugins (comma-separated list)
NEO4J_PLUGINS=["apoc", "graph-data-science"]